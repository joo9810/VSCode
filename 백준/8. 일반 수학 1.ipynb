   "source": [
    "# 2745번 진법 변환\n",
    "N, B = input().split()\n",
    "B = int(B)\n",
    "total = 0\n",
    "for i in range(len(N)):\n",
    "    if N[i].isdecimal() == True:\n",
    "        total += (int(N[i]))*(B**(len(N)-1-i))\n",
    "    else:\n",
    "        total += (ord(N[i]) - 55)*(B**(len(N)-1-i))\n",
    "\n",
    "print(total)"
   ]
